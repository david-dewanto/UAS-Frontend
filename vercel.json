{
  "routes": [
    {
      "src": "/api/proxy/test",
      "dest": "/api/proxy/test"
    },
    {
      "src": "/api/proxy/(.*)",
      "dest": "/api/proxy/[...path]"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/.*\\.(js|mjs|jsx|ts|tsx)",
      "headers": {
        "Content-Type": "application/javascript; charset=utf-8",
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}